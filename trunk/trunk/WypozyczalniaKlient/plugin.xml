               id="wypozyczalniaklient.views.ListaUzytkownikow"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               id="wypozyczalniaklient.views.Lista Wypozyczen"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               id="wypozyczalniaklient.views.plyty.ListaFilmow"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">

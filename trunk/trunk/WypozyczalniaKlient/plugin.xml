<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="wypozyczalniaklient.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="wypozyczalniaklient.Perspective"
            id="WypozyczalniaKlient.perspective">
      </perspective>
      <perspective
            class="wypozyczalniaklient.perspectives.KontaPerspektywa"
            icon="icons/releng_gears.gif"
            id="wypozyczalniaklient.perspectives.KontaPerspektywa"
            name="Konta">
      </perspective>
      <perspective
            class="wypozyczalniaklient.perspectives.WypozyczeniaPerspektywa"
            icon="icons/releng_gears.gif"
            id="wypozyczalniaklient.perspectives.WypozyczeniaPerspektywa"
            name="Wypozyczenia">
      </perspective>
      <perspective
            class="wypozyczalniaklient.perspectives.PunktyPerspektywa"
            icon="icons/releng_gears.gif"
            id="wypozyczalniaklient.perspectives.PunktyPerspektywa"
            name="Punkty">
      </perspective>
      <perspective
            class="wypozyczalniaklient.perspectives.PlytyPerspektywa"
            icon="icons/releng_gears.gif"
            id="wypozyczalniaklient.perspectives.PlytyPerspektywa"
            name="Plyty">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="WypozyczalniaKlient.application"
            name="Wypozyczalnia">
         <property
               name="windowImages"
               value="icons/shortcut-heart.GIF,icons/shortcut-heart.GIF">
         </property>
         <property
               name="appName"
               value="Wypozyczalnia">
         </property>
         <property
               name="aboutImage"
               value="icons/272.GIF">
         </property>
         <property
               name="aboutText"
               value="System zarzadzajacy siecia wypozyczalni plyt DVD">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="WypozyczalniaKlient"
